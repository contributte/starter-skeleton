{
  "name": "contributte/starter-skeleton",
  "description": "Starter project skeleton based on Nette Framework (@nette) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton",
    "web"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.1",

    "contributte/application": "^0.5.0",
    "contributte/bootstrap": "^0.6.0",
    "contributte/di": "^0.5.0",
    "contributte/forms": "^0.5.0",
    "contributte/http": "^0.4.0",
    "contributte/mail": "^0.7.0",
    "contributte/utils": "^0.5.0",
    "contributte/latte": "^0.5.0",
    "contributte/tracy": "^0.6.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
